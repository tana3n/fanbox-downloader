  {
    "manifest_version": 2,
    "name": "fanbox-downloader",
    "description": "PIXIV FANBOXの作品ページから今すぐダウンロード",
    "version": "1.2.0",
    "author": "tana3n",
    "icons": {
        "16": "icon_32.png",
        "32": "icon_32.png",
        "48": "icon_32.png",
        "128": "icon_32.png"
    },
    "content_scripts": [
        {
          "matches": ["https://*.fanbox.cc/*"],
          "js": ["fanbox-downloader.js"]
        }
      ],
      "background": {
        "scripts": ["event_page.js"]
      },
    "permissions": [
        "downloads",
        "storage",
        "tabs",
        "contextMenus",
        "https://*.fanbox.cc/posts/*",
        "https://*.fanbox.cc/*/posts/*"

    ],
    "web_accessible_resources": [
    "icon_32.png"
  ],
  "options_ui":{
    "page":"settings.html",
    "chrome_style": true


  }

}

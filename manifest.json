  {
    "manifest_version": 2,
    "name": "fanbox-downloader",
    "description": "PIXIV FANBOXの作品ページから右クリック保存",
    "version": "1.3.6",
    "author": "tana3n",
    "icons": {
        "48": "icon_48.png"
    },
    "content_scripts": [
        {
          "matches": ["https://*.fanbox.cc/*"],
          "js": ["fanbox-downloader.js"]
        }
      ],
      "background": {
        "scripts": ["event_page.js"]
      },
    "permissions": [
        "downloads",
        "storage",
        "tabs",
        "contextMenus",
        "*://*.fanbox.cc/*"
    ],
    "web_accessible_resources": [
    "icon_32.png"
  ],
  "options_ui":{
    "page":"settings.html",
    "chrome_style": true


  }

}
